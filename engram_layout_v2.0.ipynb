    "TimeSymmetric24x24 = np.ones((24,24))\n",
    "for i1, I1 in enumerate(I):\n",
    "    for i2, I2 in enumerate(I):\n",
    "        J1 = J[i1] - 1\n",
    "        J2 = J[i2] - 1\n",
    "        avgvalue = (Time24x24[I1-1,I2-1] + Time24x24[J1,J2]) / 2 \n",
